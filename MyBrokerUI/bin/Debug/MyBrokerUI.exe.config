<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="SendKeys" value="SendInput"/>
</appSettings> 

  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="MyBroker.log"/>
        <remove name="Default"/>
      </listeners>
    </trace>
  </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
